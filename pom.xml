<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.openo.oparent</groupId>
        <artifactId>oparent</artifactId>
        <version>1.0.0-SNAPSHOT</version>
        <relativePath>../oparent</relativePath>
    </parent>


    <groupId>org.openo.sdno.driver-zte-sptn</groupId>
    <artifactId>sptndriver</artifactId>
    <version>1.0.0-SNAPSHOT</version>
    <name>sptndriver</name>
    <packaging>pom</packaging>

    <modules>
        <module>models</module>
        <module>common</module>
        <module>l2vpn</module>
        <module>l3vpn</module>
        <module>application</module>
        <module>distribution</module>
    </modules>

    <properties>
        <maven.compiler.source>1.7</maven.compiler.source>
        <maven.compiler.target>1.7</maven.compiler.target>
        <java.version.source>${maven.compiler.source}</java.version.source>
        <java.version.target>${maven.compiler.target}</java.version.target>
        <maven.compile.plugin.version>3.3</maven.compile.plugin.version>
        <maven-dependency-plugin.version>2.10</maven-dependency-plugin.version>
        <maven-shade-plugin.version>2.2</maven-shade-plugin.version>
        <maven-antrun-plugin.version>1.8</maven-antrun-plugin.version>
        <maven-resources-plugin.version>2.7</maven-resources-plugin.version>

        <dropwizard.version>0.8.0</dropwizard.version>
        <swagger.version>1.5.3</swagger.version>
        <swagger-codegen-maven-plugin.version>2.2.0</swagger-codegen-maven-plugin.version>
        <swagger-annotations.version>1.5.9</swagger-annotations.version>
        <gson.version>2.7</gson.version>
        <mysql-connector-java.version>6.0.3</mysql-connector-java.version>
        <junit.version>4.10</junit.version>
        <retrofit.version>2.1.0</retrofit.version>
        <commons-net.version>3.3</commons-net.version>
    </properties>
</project>
